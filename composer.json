{
    "name": "tomk79/px2-publish-for-multi-device",
    "description": "複数のデバイス向けのバージョンをパブリッシュできる、 Pickles 2 用のパブリッシュプラグイン。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "scripts":{
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ] ,
        "documentation": [
            "php ./vendor/phpdocumentor/phpdocumentor/bin/phpdoc --title \"tomk79/px2-publish-for-multi-device API Document\" -d \"./php/\" -t \"./docs/phpdoc/\""
        ]
    } ,
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
    ],
    "require-dev": {
        "pickles2/px-fw-2.x": "dev-develop",
        "phpdocumentor/phpdocumentor": "^2.9",
        "phpunit/phpunit": "^4.8",
        "pickles2/px2-multitheme": "^2.0"
    }
}
